void merge(int* num1, int numsSize, int m, int* num2, int numsSize, int n) {
    int i = m - 1;      
    int j = n - 1;      
    int k = m + n - 1;  
    while (i >= 0 && j >= 0) {
        if (num1[i] > num2[j]) {
            num1[k] = num1[i];
            i--;
        } else {
            num[k] = num[j];
            j--;
        }
        k--;
    }
    while (j >= 0) {
        num1[k] = num2[j];
        j--;
        k--;
    }
}
